{
    "imports": {
        "/": "./hub/",
        "./": "./",
        "@": "./hub/@reframe/zero/base-runtime.ts",
        "@reframe/": "./hub/@reframe/"
    },
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "https://esm.sh/react@canary",
        "lib": [
            "esnext",
            "dom",
            "deno.ns"
        ]
    },
    "tasks": {
        "watch": "cd hub; deno run --allow-all --watch @reframe/zero/run.ts --watch=true",
        "run": "cd hub; deno run --allow-all @reframe/zero/run.ts",
        "build": "deno task run \"@reframe/one/~@/@/build.ts\"",
        "deploy": "deno task run \"@reframe/one/~@/@/deploy.ts\""
    }
}